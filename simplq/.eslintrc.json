{
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "prettier/prettier": ["error"],
        "comma-dangle": ["error", "only-multiline"],
        "no-param-reassign": ["warn"],
        "react/jsx-indent": ["warn"]
    },
    "extends": ["airbnb", "prettier", "plugin:prettier/recommended"]
}
